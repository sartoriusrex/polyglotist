{
  "name": "polyglotist-v1",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "github.com/sartoriusrex/polyglotist"
  },
  "description": "PERN (PostgresQL, Express, React, Node) app that aids intermediate language learners in learning to read new languages and learn new vocabulary.",
  "main": "src/server/index.ts",
  "scripts": {
    "exportgoogle": "export GOOGLE_APPLICATION_CREDENTIALS=\"/app/gcred.json\"",
    "typecheck": "tsc --module esnext",
    "build": "webpack --mode production --env.NODE_ENV=production --config webpack.config.cjs",
    "start": "npm run build && npm run exportgoogle && node -r ts-node/register/transpile-only src/server/index.ts",
    "client": "webpack-dev-server --config webpack.config.cjs --mode development --env.NODE_ENV=development --devtool inline-source-map --hot",
    "server": "nodemon --config config/nodemon.json src/server/index.ts",
    "dev": "npm run exportgoogle && concurrently \"npm run server\" \"npm run client\"",
    "test-c": "TS_NODE_PROJECT=config/tsconfig.json jest --coverage --config='config/jest.config.cjs'",
    "test": "TS_NODE_PROJECT=config/tsconfig.json jest --config='config/jest.config.cjs'",
    "test:watch": "jest --config='config/jest.config.cjs' --watch",
    "docker-build": "sudo docker-compose -f docker-compose.dev.yml up --build",
    "docker-start": "sudo docker-compose -f docker-compose.dev.yml up",
    "docker-prod": "sudo docker-compose up --build"
  },
  "author": "Dennis Mai",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/translate": "^6.0.0",
    "@reduxjs/toolkit": "^1.2.5",
    "@types/pg": "^7.14.4",
    "babel-polyfill": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "css.gg": "^2.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "express-rate-limit": "^5.1.1",
    "express-session": "^1.17.0",
    "express-validator": "^6.4.0",
    "helmet": "^3.22.0",
    "history": "^4.10.1",
    "jest": "^26.1.0",
    "jsonwebtoken": "^8.5.1",
    "lodash.throttle": "^4.1.1",
    "node-cron": "^2.0.3",
